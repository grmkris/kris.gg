{
  "name": "agent-next-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --bun next dev --port 3001",
    "build": "bun --bun next build",
    "start": "bun --bun next start",
    "typecheck": "tsc --noEmit",
    "fix": "ultracite fix --type-aware --type-check",
    "fix:unsafe": "ultracite fix --unsafe --type-aware --type-check",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "bun --bun src/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "test": "bun test",
    "verify": "bun run typecheck && bun run fix && bun run test"
  },
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@orpc/client": "^1.12.2",
    "@orpc/openapi": "^1.12.2",
    "@orpc/server": "^1.12.2",
    "@orpc/tanstack-query": "^1.12.2",
    "@orpc/zod": "^1.12.2",
    "@t3-oss/env-core": "^0.13.10",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tanstack/react-form": "^1.28.0",
    "@tanstack/react-query": "^5.90.12",
    "babel-plugin-react-compiler": "^1.0.0",
    "better-auth": "^1.4.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.45.1",
    "lucide-react": "^0.563.0",
    "next": "^16.1.1",
    "next-themes": "^0.4.6",
    "react": "^19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.3",
    "recharts": "2.15.4",
    "shadcn": "^3.6.2",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.4",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/bun": "^1.3.6",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "drizzle-kit": "^0.31.8",
    "oxfmt": "latest",
    "oxlint": "latest",
    "oxlint-tsgolint": "^0.11.2",
    "tailwindcss": "^4.1.10",
    "typescript": "^5",
    "ultracite": "7.0.12"
  },
  "packageManager": "bun@1.3.5"
}
